.tile {
    -webkit-perspective: 0;
    -webkit-transform-style: preserve- 3 d;
    -webkit-transition: -webkit-transform 0.2s;
    float: left;
    margin-right: 10px;
    margin-bottom: 10px;
    text-align: center;
    padding: 5px;
    opacity: 0.75;
}

    .tile h1, .tile h2, .tile h3, .tile h4, .tile h5, .tile h6 {
        color: #fff;
        -webkit-user-select: none;
    }

    .tile h2 {
        font-size: 1.75em;
        margin-top: -10px;
        margin-left: 0px;
    }

    .tile a:hover {
        text-decoration: none;
    }

    .tile img {
        border: 0;
    }

    .tile:hover, .tileNoHover {
        opacity: 1;
    }
	
    .tileNoHover {
        opacity: 1;
    }	
	
	

.tiles {
